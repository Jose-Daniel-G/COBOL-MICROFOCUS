       SELECT OPTIONAL STOCK
           ASSIGN TO "./DAT/stock.dat"
           ORGANIZATION IS INDEXED
           ACCESS MODE IS DYNAMIC
           RECORD KEY IS STK-CODIGO
           STATUS IS ST-STOCK.

       SELECT STOCK-PLANO ASSIGN TO "stock.txt"
              ORGANIZATION IS LINE SEQUENTIAL.
       SELECT STOCK-CSV   ASSIGN TO "stock.csv"
              ORGANIZATION IS LINE SEQUENTIAL.
              